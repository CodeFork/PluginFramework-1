<configuration>
  <configSections>
  </configSections>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="RabbitMQ.Client" publicKeyToken="89e7d7c5feba84ce" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.2.0" newVersion="3.0.2.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Castle.Core" publicKeyToken="407dd0808d44fbdc" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.2.0.0" newVersion="3.2.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <appSettings>
    <add key="PluginPath" value="..\..\..\..\Plugin\Bin" />
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="PluginFramework.Examples.ClientServer.WcfService">
        <endpoint address="net.tcp://localhost:54321/PluginFrameworkWcfService" binding="netTcpBinding" contract="PluginFramework.Examples.ClientServer.IWcfService">
        </endpoint>
      </service>
    </services>
  </system.serviceModel>
</configuration>